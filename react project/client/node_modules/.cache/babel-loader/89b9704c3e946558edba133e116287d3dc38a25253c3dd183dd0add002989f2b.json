how","handleClose","onHide","children","Header","closeButton","Title","fileName","_jsxFileName","lineNumber","columnNumber","Body","Footer","variant","className","modal_button","onClick","_c","$RefreshReg$"],"sources":["C:/Users/79131/OneDrive/Рабочий стол/react project/client/src/components/Footer/FooterModals/AboutSystemModal/AboutSystemModal.js"],"sourcesContent":["import React from 'react';\r\nimport { Modal, Button } from 'react-bootstrap';\r\nimport styles from '../../FooterModals/Modals.module.css'\r\n\r\nconst AboutSystemModal = ({ show, handleClose }) => {\r\n  return (\r\n    <Modal  show={show} onHide={handleClose}>\r\n      <Modal.Header closeButton>\r\n        <Modal.Title>О системе</Modal.Title>\r\n      </Modal.Header>\r\n      <Modal.Body >\r\n        <p>\r\n          Разработанная система является веб-приложением с клиент-серверной архитектурой с использованием методов искусственного интеллекта. Веб-приложение предназначено для автоматизации процессов государственной итоговой аттестации (ГИА) по образовательным программам высшего образования - бакалавриата, специалитета и магистратуры. Пользователями системы являются выпускники, руководители, секретарь государственной экзаменационной комиссии. Система является решением, позволяющим минимизировать количество ошибок и сократить временные затраты при заполнении документации, требуемой для организации процессов ГИА, инструментом коммуникации между участниками процессов ГИА, средством повышения информированности выпускников о правилах проведения процедуры ГИА. При разработке системы учтена возможность изменений регламента проведения ГИА, форм документов и иных аспектов процедуры.\r\n        </p>\r\n        <p>Версия системы: 1.0</p>\r\n      </Modal.Body>\r\n      <Modal.Footer>\r\n        <Button variant=\"none\" className={styles.modal_button} onClick={handleClose}>\r\n          Закрыть\r\n        </Button>\r\n      </Modal.Footer>\r\n    </Modal>\r\n  );\r\n};\r\n\r\nexport default AboutSystemModal;\r\n"],"mappings":";AAAA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,KAAK,EAAEC,MAAM,QAAQ,iBAAiB;AAC/C,OAAOC,MAAM,MAAM,sCAAsC;AAAA,SAAAC,MAAA,IAAAC,OAAA;AAEzD,MAAMC,gBAAgB,GAAGA,CAAC;EAAEC,IAAI;EAAEC;AAAY,CAAC,KAAK;EAClD,oBACEH,OAAA,CAACJ,KAAK;IAAEM,IAAI,EAAEA,IAAK;IAACE,MAAM,EAAED,WAAY;IAAAE,QAAA,gBACtCL,OAAA,CAACJ,KAAK,CAACU,MAAM;MAACC,WAAW;MAAAF,QAAA,eACvBL,OAAA,CAACJ,KAAK,CAACY,KAAK;QAAAH,QAAA,EAAC;MAAS;QAAAI,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAa;IAAC;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OACxB,CAAC,eACfZ,OAAA,CAACJ,KAAK,CAACiB,IAAI;MAAAR,QAAA,gBACTL,OAAA;QAAAK,QAAA,EAAG;MAEH;QAAAI,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAG,CAAC,eACJZ,OAAA;QAAAK,QAAA,EAAG;MAAmB;QAAAI,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAG,CAAC;IAAA;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAChB,CAAC,eACbZ,OAAA,CAACJ,KAAK,CAACkB,MAAM;MAAAT,QAAA,eACXL,OAAA,CAACH,MAAM;QAACkB,OAAO,EAAC,MAAM;QAACC,SAAS,EAAElB,MAAM,CAACmB,YAAa;QAACC,OAAO,EAAEf,WAAY;QAAAE,QAAA,EAAC;MAE7E;QAAAI,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAQ;IAAC;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OACG,CAAC;EAAA;IAAAH,QAAA,EAAAC,YAAA;IAAAC,UAAA;IAAAC,YAAA;EAAA,OACV,CAAC;AAEZ,CAAC;AAACO,EAAA,GAnBIlB,gBAAgB;AAqBtB,eAAeA,gBAAgB;AAAC,IAAAkB,EAAA;AAAAC,YAAA,CAAAD,EAAA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}